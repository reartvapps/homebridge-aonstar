{
  "name": "homebridge-onstar-dev",
  "version": "1.0.62",
  "description": "Unofficial OnStar plugin for homebridge.",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "type": "module",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "rm -rf dist && rollup -c",
    "dev": "rm -rf dist && rollup -c -w",
    "lint": "prettier --write .",
    "prepublishOnly": "pnpm run build",
    "release": "standard-version --sign",
    "test": "pnpm test:unit && pnpm test:functional",
    "test:unit": "jest test/unit",
    "test:coverage": "jest test/unit --coverage",
    "test:functional": "jest test/functional",
    "test:keys": "jest test/functional -t 'Unupgraded Command Successful'",
    "test:auth": "jest test/auth/testAuth",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts,md,yml}": [
      "prettier --write"
    ]
  },
  "packageManager": "pnpm@9.14.4",
  "standard-version": {
    "scripts": {
      "prerelease": "pnpm test:unit && pnpm build"
    }
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reartvapps/homebridge-onstar-dev.git"
  },
  "keywords": [
    "onstar",
    "chevrolet",
    "chevy",
    "gm",
    "gmc",
    "buick",
    "cadillac"
  ],
  "author": "me",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reartvapps/homebridge-onstar-dev/issues"
  },
  "homepage": "https://github.com/homebridge-onstar-dev/#readme",
  "dependencies": {
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "axios": "^1.7.8",
    "http-cookie-agent": "^6.0.6",
    "jsonwebtoken": "^9.0.2",
    "openid-client": "^5.7.1",
    "totp-generator": "^1.0.0",
    "tough-cookie": "^5.0.0",
    "uuid": "^11.0.3"
  }
}
